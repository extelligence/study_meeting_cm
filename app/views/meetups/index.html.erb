<h1>Listing meetups: <%= current_staff.name %>さんのマイページ</h1>

<p><%= link_to 'New Meetup', new_meetup_path %></p>

<table>
  <tr>
    <th>日付</th>
    <th>イベント名</th>
    <th>CM申込数</th>
    <th>ステータス</th>
    <th></th>
    <th></th>
  </tr>

<% @meetups.each do |meetup| %>
  <tr>
    <td><%= meetup.date %></td>
    <td><%= link_to meetup.name, meetup %></td>
    <td>0</td>
    <td><%= meetup.display_status %></td>
    <td><%= link_to '変更', edit_meetup_path(meetup) %></td>
    <td><%= link_to '中止', meetup, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
